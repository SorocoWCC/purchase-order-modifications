        <field name="view_type">form</field>
        <field name="model">impresora</field>
        <field name="arch" type="xml">
          <form string="Configuracion Impresora"  version="7.0">
            <header>  

            </header>
            <sheet string="Configuracion Impresora">
              <group >      
                <group>
                  <field  name="name"  />
